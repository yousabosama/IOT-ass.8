#include <stdio.h>

int stringLength(char *str)
{
    char *ptr = str;
    int length = 0;

    while (*ptr != '\0') 
    {
        length++;
        ptr++;
    }

    return length;
}

int main() {
    char str[] = "Hello";
    int len = stringLength(str);
    printf("Length = %d\n", len);
    return 0;
}
